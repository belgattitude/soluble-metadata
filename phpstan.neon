includes:
	- vendor/phpstan/phpstan-strict-rules/rules.neon
	- vendor/phpstan/phpstan-phpunit/extension.neon
	- vendor/phpstan/phpstan-phpunit/rules.neon
parameters:
	fileExtensions:
		- php
	reportUnmatchedIgnoredErrors: true
	ignoreErrors:
		- message: '#Array has 2 duplicate keys with value 1 \(MYSQLI_TYPE_CHAR, MYSQLI_TYPE_TINY\)#'
		  path: src/Soluble/Metadata/Reader/Mapping/MysqliMapping.php

